<configuration>
	<conferences>
		<conference name="iswc2012">
			
			<logoUri>http://iswc2012.semanticweb.org/sites/default/files/iswc_logo.jpg</logoUri>
			<baseUri>http://data.semanticweb.org/</baseUri>
			 <confId>conference/iswc/2012</confId>
			  <tracks>
				    <track>
